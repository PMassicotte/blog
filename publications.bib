@article{Staehr2002,
  title = {Photoacclimation of Four Marine Phytoplankton Species to Irradiance and Nutrient Availability},
  author = {Staehr, {\relax PA} and Henriksen, P and Markager, S},
  year = {2002},
  journal = {Marine Ecology Progress Series},
  volume = {238},
  pages = {47--59},
  issn = {0171-8630},
  doi = {10.3354/meps238047},
  abstract = {Photoacclimation to different light and nutrient regimes was studied in 4 common marine phytoplankton species from northern European coastal waters. Significant changes were observed for all species in their cellular nutrient content, pigment packaging and composition, and ch1 a-specific in vivo light absorption (a*(ch1a)(lambda)). From exponential to stationary growth, the C:N and carotenoid:chl a ratios increased, while the cellular ch1 a content decreased. All phytoplankton species acclimated to increasing irradiance by reducing their cellular ch1 a content and increasing the carotenoid:chl a ratio, resulting in higher ch1 a-specific absorption coefficients (a*(ch1a)). Similar results were observed for N-depleted cultures. Although acclimation to different irradiance and nutrient conditions introduced a substantial variability in pigment composition and intracellular pigment concentration, significant interspecies differences in a*(ch1a) were still apparent. The interspecies differences were primarily due to differences in cell size, which covaried with pigment packaging and composition. On this basis, we suggest that optical acclimation (i.e. changes in pigment composition and packaging) to the prevailing growth conditions (light, temperature and nutrient availability) is less important than differences in phytoplankton composition (dominant cell size and pigment composition) for the variation in a*(ch1a)(lambda) of natural phytoplankton communities.},
  isbn = {10.3354/meps238047},
  langid = {english},
  keywords = {chl a-specific in vivo light absorption photoaccli,dunaliella-tertiolecta,fluorescence,growth,heterocapsa sp,light-absorption,natural phytoplankton,nitrogen,north-atlantic,phytoplankton,pigment distribution,pigmentation,spectral absorption-coefficients},
