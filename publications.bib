  langid = {english},
  keywords = {absorption,aquatic ecosystems,b penetration,carbon,chromophoric dissolved organic matter (cdom),diffuse attenuation coefficient,fluorescence,humic substances,optical-properties,seasonal variability,solar uv-radiation,uv-b radiation,waters,yellow substance,yunnan plateau}
