@article{Grob2007,
  title = {Contribution of Picoplankton to the Total Particulate Organic Carbon Concentration in the Eastern {{South Pacific}}},
  author = {Grob, C. and Ulloa, O. and Claustre, H. and Huot, Y. and Alarc{\'o}n, G. and Marie, D.},
