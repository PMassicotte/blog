@article{Chapman2010,
  title = {Changes in Water Colour between 1986 and 2006 in the Headwaters of the {{River Nidd}}, {{Yorkshire}}, {{UK}}},
  author = {Chapman, Pippa J and McDonald, Adrian T. and Tyson, Rosa and Palmer, Sheila M and Mitchell, Gordon and Irvine, Brian},
