}

@article{Staehr2004,
  title = {Parameterization of the Chlorophyll a -Specific in Vivo Light Absorption Coefficient Covering Estuarine, Coastal and Oceanic Waters},
  author = {St{\ae}hr, P. A. and Markager, S},
  year = {2004},
