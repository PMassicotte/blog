@incollection{Sharp2002,
  title = {Chapter 2 - Analytical Methods for Total \{\vphantom\}{{DOM}}\vphantom\{\} Pools},
  booktitle = {Biogeochemistry of Marine Dissolved Organic Matter},
  author = {Sharp, Jonathan H},
  editor = {Carlson, Dennis A HansellCraig A},
  year = {2002},
  pages = {35--58},
  publisher = {{Academic Press}},
  address = {{San Diego}},
  doi = {http://dx.doi.org/10.1016/B978-012323841-2/50004-X},
  abstract = {Publisher Summary This chapter discusses several analytical methods for total dissolving organic matter (DOM) pools. The main reason for measuring \{DOC\} has been to quantify the total DOM. The measurement focuses on ``dissolved'' organic matter with the assumption that particles can be removed by filtration through a micro-porous filter. Such filtration\textemdash essential in near shore waters\textemdash is often omitted in oceanic waters where the total organic matter is almost all ``dissolved.'' For over a century, there has been an interest in quantifying the pool of \{DOM\} in the sea and in other aquatic environments. Some of the early efforts included attempts to quantify ``humic substances'' and to measure colored material. Specific organic compounds have been measured and attempts have been made to understand origins and fates of these compounds for decades. While that type of research is possibly more interesting and fruitful\textemdash depending upon the questions asked\textemdash more energy has been expended overall to quantify the gross amount of DOM.},
  isbn = {978-0-12-323841-2}
}

@misc{Sharpless2014,
  title = {The Importance of Charge-Transfer Interactions in Determining Chromophoric Dissolved Organic Matter ({{CDOM}}) Optical and Photochemical Properties},
  author = {Sharpless, Charles M. and Blough, Neil V.},
  year = {2014},
  month = mar,
  journal = {Environmental sciences: Processes and impacts},
  volume = {16},
  number = {4},
  pages = {654--671},
  publisher = {{The Royal Society of Chemistry}},
  issn = {20507895},
  doi = {10.1039/c3em00573a},
  abstract = {A critical review presenting the case for an electronic interaction model as the basis for CDOM optical and photochemical properties.Absorption of sunlight by chromophoric dissolved natural organic matter (CDOM) is environmentally significant because it controls photic zone depth and causes photochemistry that affects elemental cycling and contaminant fate. Both the optics (absorbance and fluorescence) and photochemistry of CDOM display unusual properties that cannot easily be ascribed to a superposition of individual chromophores. These include (i) broad, unstructured absorbance that decreases monotonically well into the visible and near IR, (ii) fluorescence emission spectra that all fall into a single envelope regardless of the excitation wavelength, and (iii) photobleaching and photochemical quantum yields that decrease monotonically with increasing wavelength. In contrast to a simple superposition model, these phenomena and others can be reasonably well explained by a physical model in which charge-transfer interactions between electron donating and accepting chromophores within the CDOM control the optical and photophysical properties. This review summarizes current understanding of the processes underlying CDOM photophysics and photochemistry as well as their physical basis.},
  keywords = {\#duplicate-citation-key}
}

