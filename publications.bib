@article{Nurminen2002,
  title = {A Diurnal Study on the Distribution of Filter Feeding Zooplankton: {{Effect}} of Emergent Macrophytes, {{pH}} and Lake Trophy},
  author = {Nurminen, Leena K L and Horppila, Jukka A.},
  year = {2002},
