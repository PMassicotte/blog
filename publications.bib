  issn = {2167-8359},
  doi = {10.7717/peerj.760},
  abstract = {Environmental homogenization in coastal ecosystems impacted by human activities may be an important factor explaining the observed decline in fish species richness. We used fish community data ({$>$}200 species) from extensive surveys conducted in two biogeographic provinces (extent {$>$}1,000 km) in North America to quantify the relationship between fish species richness and local (grain {$<$}10 km\textbackslash textsuperscript\{2\}) environmental heterogeneity. Our analyses are based on samples collected at nearly 800 stations over a period of five years. We demonstrate that fish species richness in coastal ecosystems is associated locally with the spatial heterogeneity of environmental variables but not with their magnitude. The observed effect of heterogeneity on species richness was substantially greater than that generated by simulations from a random placement model of community assembly, indicating that the observed relationship is unlikely to arise from veil or sampling effects. Our results suggest that restoring or actively protecting areas of high habitat heterogeneity may be of great importance for slowing current trends of decreasing biodiversity in coastal ecosystems.},
  copyright = {All rights reserved},
  keywords = {Aquatic community assembly,Conservation biology,Diversity,Heterogeneity hypothesis,Random placement model,Simulation},
  file = {/home/filoche/Zotero/storage/EJD5C9AC/Massicotte et al. - 2015 - Testing the influence of environmental heterogeneity on fish species richness in two biogeographic provinc(2).pdf}
}

@article{Massicotte2015a,
  title = {Influence of the Aquatic Vegetation Landscape on Larval Fish Abundance},
  author = {Massicotte, Philippe and Bertolo, Andrea and Brodeur, Philippe and Hudon, Christiane and Mingelbier, Marc and Magnan, Pierre},
  year = {2015},
