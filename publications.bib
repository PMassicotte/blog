@article{Cuevas2011,
  title = {Organic Carbon and Mineral Nutrient Limitation of Oxygen Consumption, Bacterial Growth and Efficiency in the {{Norwegian Sea}}},
  author = {Cuevas, L. Antonio and Egge, Jorun K. and Thingstad, T. Frede and T{\"o}pper, Birte},
