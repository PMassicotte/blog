@article{Rietkerk2008,
  title = {Regular Pattern Formation in Real Ecosystems},
  author = {Rietkerk, Max and {van de Koppel}, Johan},
  year = {2008},
  journal = {Trends in ecology and evolution},
  volume = {23},
  number = {February},
  pages = {169--175},
  issn = {01695347},
  doi = {10.1016/j.tree.2007.10.013},
  abstract = {Localized ecological interactions can generate striking large-scale spatial patterns in ecosystems through spatial self-organization. Possible mechanisms include oscillating consumer-resource interactions, localized disturbance-recovery processes and scale-dependent feedback. Despite abundant theoretical literature, studies revealing spatial self-organization in real ecosystems are limited. Recently, however, many examples of regular pattern formation have been discovered, supporting the importance of scale-dependent feedback. Here, we review these studies, showing regular pattern formation to be a general phenomenon rather than a peculiarity. We provide a conceptual framework explaining how scale-dependent feedback determines regular pattern formation in ecosystems. More empirical studies are needed to better understand regular pattern formation in ecosystems, and how this affects the response of ecosystems to global environmental change. ?? 2007 Elsevier Ltd. All rights reserved.},
  isbn = {0169-5347},
  pmid = {18255188}
}

