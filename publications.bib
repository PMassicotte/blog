@article{Osburn2009a,
  title = {Photoreactivity of Chromophoric Dissolved Organic Matter Transported by the {{Mackenzie River}} to the {{Beaufort Sea}}},
  author = {Osburn, Christopher L. and Retamal, Leira and Vincent, Warwick F.},
