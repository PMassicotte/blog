@article{Kowalczuk2006,
  title = {Modeling Absorption by {{CDOM}} in the {{Baltic Sea}} from Season, Salinity and Chlorophyll},
  author = {Kowalczuk, P and Stedmon, C A and Markager, S},
  year = {2006},
  journal = {Marine Chemistry},
  volume = {101},
  pages = {1--11 ST -- Modeling absorption by CDOM in the Balt},
  address = {{Kowalczuk, P Polish Acad Sci, Inst Oceanol, Ul Powstancow Warzawy 55, PL-81712 Sopot, Poland Polish Acad Sci, Inst Oceanol, PL-81712 Sopot, Poland Natl Environm Res Inst, Dept Marine Ecol, DK-4000 Roskilde, Denmark}},
  issn = {0304-4203},
  abstract = {In the Baltic Sea the underwater light regime is to a large extent governed by absorption by colored dissolved organic matter (CDOM), where the largest source is riverine input. We have applied a conservative mixing model of CDOMs optical properties to a dataset collected in the Baltic Sea during 11 years of continuous observations in 1993-2004. The majority of observations agreed well with the model indicating that conservative mixing is important in the area, as terrestrial organic matter is diluted with open sea water. Deviations from the conservative mixing pattern mainly occurred at salinities over 6.8, and chlorophyll a concentrations 3 over 1.5 mg m(-3), and were located in open sea waters, coastal zone and Pomeranian Bay. The seasonal dependence between the light absorption coefficient by CDOM a(CDOM)(375) and salinity and chlorophyll a concentrations was explored. In March, April and November, months of intensive mixing and high riverine discharge, most of the variability in a(CDOM)(375) values could be explained by dilution of terrestrially derived CDOM alone. In February, May, and September, months of thermal stratification, reduced riverine discharge and enhanced biological activity, inclusion of chlorophyll a concentration resulted in significantly better models. Autochthonous production of CDOM was found to be a significant source of CDOM in the Southern Baltic Sea in these months. A series of algorithms for the prediction of CDOMs optical properties is presented. (c) 2005 Elsevier B.V. All rights reserved.},
  keywords = {colored dissolved organic matter optical propertie}
}

@article{Kowalczuk2010,
  title = {Characterization of Dissolved Organic Matter Fluorescence in the {{South Atlantic Bight}} with Use of {{PARAFAC}} Model: {{Relationships}} between Fluorescence and Its Components, Absorption Coefficients and Organic Carbon Concentrations},
