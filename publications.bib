  author = {{van de Poll}, W. H. and Kulk, G. and Timmermans, K. R. and Brussaard, C. P. D. and {van der Woerd}, H. J. and Kehoe, M. J. and Mojica, K. D. A. and Visser, R. J. W. and Rozema, P. D. and Buma, A. G. J.},
  year = {2013},
