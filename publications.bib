@article{Raymond2007,
  title = {Flux and Age of Dissolved Organic Carbon Exported to the {{Arctic Ocean}}: {{A}} Carbon Isotopic Study of the Five Largest Arctic Rivers},
  author = {Raymond, Peter A. and McClelland, J. W. and Holmes, R. M. and Zhulidov, A. V. and Mull, K. and Peterson, B. J. and Striegl, R. G. and Aiken, G. R. and Gurtovaya, T. Y.},
