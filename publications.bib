@article{Reader2015,
  title = {Mass and {{UV-visible}} Spectral Fingerprints of Dissolved Organic Matter: Sources and Reactivity},
  author = {Reader, Heather Erin and a. Stedmon, Colin and Nielsen, Nikoline J. and Kritzberg, Emma S.},
  year = {2015},
  month = oct,
  journal = {Frontiers in Marine Science},
  volume = {2},
