@article{Schaefer2014,
  title = {The Impact of the Permafrost Carbon Feedback on Global Climate},
  author = {Schaefer, Kevin and Lantuit, Hugues and Romanovsky, Vladimir E. and Schuur, Edward A.G. and Witt, Ronald},
  year = {2014},
  journal = {Environmental Research Letters},
  volume = {9},
  number = {8},
  pages = {85003},
  issn = {17489326},
  doi = {10.1088/1748-9326/9/8/085003},
  abstract = {Degrading permafrost can alter ecosystems, damage infrastructure, and release enough carbon dioxide (CO\textexclamdown inf\textquestiondown 2\textexclamdown/inf\textquestiondown ) and methane (CH\textexclamdown inf\textquestiondown 4\textexclamdown/inf\textquestiondown ) to influence global climate. The permafrost carbon feedback (PCF) is the amplification of surface warming due to CO\textexclamdown inf\textquestiondown 2\textexclamdown/inf\textquestiondown{} and CH\textexclamdown inf\textquestiondown 4\textexclamdown/inf\textquestiondown{} emissions from thawing permafrost. An analysis of available estimates PCF strength and timing indicate 120{$\alpha$}85 Gt of carbon emissions from thawing permafrost by 2100. This is equivalent to 5.7{$\alpha$}4.0\% of total anthropogenic emissions for the Intergovernmental Panel on Climate Change (IPCC) representative concentration pathway (RCP) 8.5 scenario and would increase global temperatures by 0.29{$\alpha$}0.21 \textdegree C or 7.8{$\alpha$}5.7\%. For RCP4.5, the scenario closest to the 2 \textdegree C warming target for the climate change treaty, the range of cumulative emissions in 2100 from thawing permafrost decreases to between 27 and 100 Gt C with temperature increases between 0.05 and 0.15 \textdegree C, but the relative fraction of permafrost to total emissions increases to between 3\% and 11\%. Any substantial warming results in a committed, long-term carbon release from thawing permafrost with 60\% of emissions occurring after 2100, indicating that not accounting for permafrost emissions risks overshooting the 2 \textdegree C warming target. Climate projections in the IPCC Fifth Assessment Report (AR5), and any emissions targets based on those projections, do not adequately account for emissions from thawing permafrost and the effects of the PCF on global climate. We recommend the IPCC commission a special assessment focusing on the PCF and its impact on global climate to supplement the AR5 in support of treaty negotiation.},
  keywords = {global climate,permafrost,permafrost carbon feedback}
}

