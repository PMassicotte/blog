@article{Bouillon2014,
  title = {Contrasting Biogeochemical Characteristics of the {{Oubangui River}} and Tributaries ({{Congo River}} Basin)},
  author = {Bouillon, Steven and Yamb{\'e}l{\'e}, Athanase and Gillikin, David P and Teodoru, Cristian and Darchambeau, Fran{\c c}ois and Lambert, Thibault and Borges, Alberto V},
  year = {2014},
  month = jun,
  journal = {Scientific Reports},
  volume = {4},
  pages = {5402},
  issn = {2045-2322},
  doi = {10.1038/srep05402},
  abstract = {The Oubangui is a major tributary of the Congo River. We describe the biogeochemistry of contrasting tributaries within its central catchment, with watershed vegetation ranging from wooded savannahs to humid rainforest. Compared to a 2-year monitoring record on the mainstem Oubangui, these tributaries show a wide range of biogeochemical signatures, from highly diluted blackwaters (low turbidity, pH, conductivity, and total alkalinity) in rainforests to those more typical for savannah systems. Spectral analyses of chromophoric dissolved organic matter showed wide temporal variations in the Oubangui compared to spatio-temporal variations in the tributaries, and confirm that different pools of dissolved organic carbon are mobilized during different hydrological stages. {$\delta$}(13)C of dissolved inorganic carbon ranged between -28.1\textperthousand{} and -5.8\textperthousand, and was strongly correlated to both partial pressure of CO2 and to the estimated contribution of carbonate weathering to total alkalinity, suggesting an important control of the weathering regime on CO2 fluxes. All tributaries were oversaturated in dissolved greenhouse gases (CH4, N2O, CO2), with highest levels in rivers draining rainforest. The high diversity observed underscores the importance of sampling that covers the variability in subcatchment characteristics, to improve our understanding of biogeochemical cycling in the Congo Basin.},
  isbn = {2045-2322},
  pmid = {24954525},
  file = {/home/filoche/Zotero/storage/Z4JMH2QT/Bouillon et al. - 2014 - Contrasting biogeochemical characteristics of the Oubangui River and tributaries (Congo River basin)(2).pdf}
}

