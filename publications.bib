@article{Romera-Castillo2010,
  title = {Production of Chromophoric Dissolved Organic Matter by Marine Phytoplankton},
  author = {{Romera-Castillo}, C and Sarmento, H and {Alvarez-Salgado}, X A and Gasol, J M and Marrase, C},
