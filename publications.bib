@article{Balzano2017,
  title = {Morphological and Genetic Diversity of {{Beaufort Sea}} Diatoms with High Contributions from the {{Chaetoceros}} Neogracilis Species Complex},
  author = {Balzano, Sergio and Percopo, Isabella and Siano, Raffaele and Gourvil, Priscillia and Chanoine, M{\'e}lanie and Marie, Dominique and Vaulot, Daniel and Sarno, Diana},
