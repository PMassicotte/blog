  langid = {english},
  keywords = {fluorescence,humic substances,irradiation,light,matter,ozone depletion,photodegradation,phytoplankton,ultraviolet-radiation,water},
  file = {/home/filoche/Zotero/storage/J53VSDW6/Morris, Hargreaves - 1997 - The role of photochemical degradation of dissolved organic carbon in regulating the UV transparency of th(2).pdf}
