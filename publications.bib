@article{Bravo2009,
  title = {Nitrate Determination in {{Chilean Caliche}} Samples by {{UV-visible}} Absorbance Measurements and Multivariate Calibraron},
  author = {Bravo, Manuel and Olwiert, Alejandro C. and Oelckers, Beatriz},
  year = {2009},
  journal = {Journal of the Chilean Chemical Society},
  volume = {54},
