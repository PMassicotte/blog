@article{Retamal2007,
  title = {Comparison of the Optical Properties of Dissolved Organic Matter in Two River-Influenced Coastal Regions of the {{Canadian Arctic}}},
  author = {Retamal, Leira and Vincent, Warwick F. and Martineau, Christine and Osburn, Christopher L.},
  year = {2007},
