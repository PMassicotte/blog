@article{Ambo-Rappe2013,
  title = {Relationship between the Tropical Seagrass Bed Characteristics and the Structure of the Associated Fish Community},
  author = {{Ambo-Rappe}, Rohani and Nessa, Muhammad Natsir and Latuconsina, Husain and Lajus, Dmitry L.},
