@article{Aiazzi2005,
  title = {Information-Theoretic Heterogeneity Measurement for {{SAR}} Imagery},
  author = {Aiazzi, B and Alparone, L and Baronti, S},
  year = {2005},
  journal = {Ieee Transactions on Geoscience and Remote Sensing},
  volume = {43},
  pages = {619--624 ST -- Information--theoretic heterogeneity},
  address = {{Aiazzi, B Nello Carrara Natl Res Council, Inst Appl Phys, I-50127 Florence, Italy Nello Carrara Natl Res Council, Inst Appl Phys, I-50127 Florence, Italy Univ Florence, Dept Elect \& Telecommun, I-50139 Florence, Italy}},
  issn = {0196-2892},
  abstract = {In this communication, a heterogeneity feature, calculable from synthetic aperture radar (SAR) images on a per-pixel basis, but relying on global image statistics, is defined and discussed. Starting from the multiplicative speckle and texture models relating the amount of texture and speckle to the local mean and variance at every pixel, such a feature is rigorously derived from Shannon's information theory as the conditional information of local standard deviation to local mean. Thanks to robust statistical estimation, it is very little sensitive to the noise affecting SAR data, and thus capable of capturing subtle variations of texture whenever they are embedded in a heavy speckle. Experimental results carried out on two SAR images with different degrees of noisiness demonstrate that the proposed feature is likely to be useful for a variety of automated segmentation and classification tasks.},
  keywords = {information-theoretic signal,processing land cover}
}

