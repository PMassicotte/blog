@article{Weishaar2003a,
  title = {Evaluation of Specific Ultraviolet Absorbance as an Indicator of the Chemical Composition and Reactivity of Dissolved Organic Carbon},
  author = {Weishaar, James L and Aiken, George R and Bergamaschi, Brian A and Fram, Miranda S and Fujii, Roger and Mopper, Kenneth},
