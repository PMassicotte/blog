@article{Nelson2010,
  title = {Tracing Global Biogeochemical Cycles and Meridional Overturning Circulation Using Chromophoric Dissolved Organic Matter},
  author = {Nelson, Norman B. and Siegel, David A. and Carlson, Craig A and Swan, Chantal M.},
  year = {2010},
