  langid = {english},
  keywords = {allochthony,c-13,c-13 addition,dynamics,exchange,food web,humic lakes,metabolism,model,northern sweden,oligotrophic lake,stable-isotope analysis,subsidies,subsidy,whole-lake experiments,zooplankton},
